      <span style="letter-spacing:initial;margin:0;padding:0;text-align:center;">Your WeDeploy account is going away on April 22, 2019.<a href="/blog/discontinuing-wedeploy/" style="color:inherit;border-bottom:solid 1px rgba(14,20,26,0.3);line-height:24px;margin-left:6px;padding-bottom:1px;text-decoration:none;">Learn more</a></span>
    </div>
    <div style="position:relative">
      {call Header.render data="all" /}
      <main>
        {call Start.render data="all" /}
        {call Highlights.render data="all" /}
        {call Possibilities.render data="all" /}
        {call PlugNPlay.render data="all" /}
        {call Data.render data="all" /}
        {call DataDemo.render data="all" /}
        {call Email.render data="all" /}
        {call Auth.render data="all" /}
        {call Hosting.render data="all" /}
        {call Pricing.render data="all" /}
        {call Enterprise.render data="all" /}
        {call Benefits.render data="all" /}
        {call Answers.render data="all" /}
        {call Support.render data="all" /}
        {call Try.render data="all" /}
        {call Footer.render data="all" /}
        {call .intercom /}
        {call .googleAnalytics /}
      </main>
    </div>
  </div>
{/template}

